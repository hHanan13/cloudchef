{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.3|^8.0",
        "ext-gd": "*",
        "appstract/laravel-stock": "^1.2",
        "barryvdh/laravel-dompdf": "^2.0",
        "barryvdh/laravel-translation-manager": "^0.6.3",
        "biscolab/laravel-recaptcha": "^5.3",
        "botman/botman": "^2.7",
        "botman/driver-web": "^1.5",
        "dompdf/dompdf": "^2.0",
        "fruitcake/laravel-cors": "^3.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.7",
        "ixudra/curl": "^6.22",
        "jrean/laravel-user-verification": "^10.0",
        "laravel/framework": "^9.10",
        "laravel/passport": "^10.4",
        "laravel/sanctum": "^2.11",
        "laravel/scout": "^9.4",
        "laravel/tinker": "^2.5",
        "laravel/ui": "^3.4",
        "league/flysystem-aws-s3-v3": "^3.0",
        "livewire/livewire": "^2.10",
        "maatwebsite/excel": "*",
        "mcamara/laravel-localization": "^1.7",
        "munafio/chatify": "^1.5",
        "nesbot/carbon": "^2.63",
        "power-components/livewire-powergrid": "^3.1",
        "psr/simple-cache": "2.0",
        "pusher/pusher-php-server": "^7.0",
        "realrashid/sweet-alert": "^5.1",
        "spatie/laravel-flash": "^1.9",
        "spatie/laravel-multitenancy": "^2.0",
        "spatie/laravel-permission": "^5.5",
        "stevebauman/location": "^6.2"
    },
    "require-dev": {
        "spatie/laravel-ignition": "^1.0",
        "fakerphp/faker": "^1.9.1",
        "laravel/sail": "^1.0.1",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files":[
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "platform-check":false,
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
